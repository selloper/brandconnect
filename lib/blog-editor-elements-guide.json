                "gif": {
                  "selector": ".se-tab-button-gif",
                  "description": "움직이는 GIF 스티커 탭"
                },
                "normal": {
                  "selector": ".se-tab-button:not(.se-tab-button-history):not(.se-tab-button-gif)",
                  "description": "일반 스티커 탭"
                }
              }
            },
            "stickerList": {
              "container": ".se-sidebar-list",
              "activeContainer": ".se-sidebar-list.se-is-on",
              "items": {
                "gif": {
                  "selector": ".se-sidebar-element-sticker-gif",
                  "description": "움직이는 GIF 스티커"
                },
                "normal": {
                  "selector": ".se-sidebar-element-sticker",
                  "description": "일반 스티커"
                }
              }
            }
          },
          "process": {
            "step1": {
              "action": "click",
              "selector": ".se-sticker-toolbar-button",
              "description": "스티커 버튼 클릭하여 패널 열기"
            },
            "step2": {
              "action": "wait",
              "selector": ".se-sidebar-container-sticker",
              "description": "스티커 패널이 열릴 때까지 대기"
            },
            "step3": {
              "action": "findTabs",
              "context": "iframe",
              "selector": ".se-tab-item button",
              "description": "iframe 내에서 탭 버튼들 찾기"
            },
            "step4": {
              "action": "selectRandomTab",
              "exclude": 2,
              "description": "마지막 2개 탭(설정, 마켓) 제외하고 랜덤 선택"
            },
            "step5": {
              "action": "waitForActiveList",
              "selector": ".se-sidebar-list.se-is-on",
              "description": "선택된 탭의 스티커 목록이 활성화될 때까지 대기"
            },
            "step6": {
              "action": "findStickers",
              "context": "activeList",
              "selectors": [".se-sidebar-element-sticker-gif", ".se-sidebar-element-sticker"],
              "description": "활성화된 목록에서 스티커 찾기"
            },
            "step7": {
              "action": "selectRandomSticker",
              "description": "찾은 스티커 중 랜덤으로 하나 선택하여 클릭"
            }
          }
        }
      },
      "implementation": {
        "moduleFunction": "addSticker",
        "modulePath": "./modules/sticker-module",
        "fileName": "sticker-module.js",
        "usage": "await addSticker(page, frame);",
        "description": "랜덤 탭 선택 후 랜덤 스티커를 선택하여 추가하는 모듈 함수",
        "codeSnippet": "// 스티커 버튼 클릭 → 탭 찾기 → 랜덤 탭 선택 → 스티커 목록 로드 대기 → 활성화된 목록에서 스티커 찾기 → 랜덤 스티커 클릭\nawait frame.click('.se-sticker-toolbar-button');\nconst tabButtons = await frame.$$('.se-tab-item button');\nconst randomTabIndex = Math.floor(Math.random() * (tabButtons.length - 2));\nawait tabButtons[randomTabIndex].click();\nawait frame.waitForSelector('.se-sidebar-list.se-is-on');\nconst activeList = await frame.$('.se-sidebar-list.se-is-on');\nconst stickerItems = await activeList.$$('.se-sidebar-element-sticker-gif');\nconst randomSticker = Math.floor(Math.random() * stickerItems.length);\nawait stickerItems[randomSticker].click();"
      }
    },
