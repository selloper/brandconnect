            },
            "tabs": {
              "selector": ".se-tab-item button",
              "totalCount": 10,
              "validCount": 8,
              "excludedTabs": ["설정", "OGQ마켓"],
              "types": {
                "history": {
                  "selector": ".se-tab-button-history",
